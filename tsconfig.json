{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es3",
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    // "isolatedModules": true,
    "declaration": false,
    "downlevelIteration": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  }
}
